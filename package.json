    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^4.0.18",
    "vitest-browser-svelte": "^2.0.2"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "better-sqlite3": "^12.6.2",
    "svelte-adapter-bun": "^1.0.1",
    "valibot": "^1.2.0"
