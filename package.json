    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.19.11",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.9",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.39.2",
